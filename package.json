{
  "name": "spring-react-redux-universal-example",
  "version": "0.0.1-SNAPSHOT",
  "description": "Example of The Universal Web Application using Spring Boot + React + Redux.",
  "scripts": {
    "postinstall": "npm-sort",
    "slate": "rm -rf node_modules && npm install && npm-sort",
    "clean": "rm -rf src/main/resources/static/dist && npm-sort",
    "dev": "NODE_ENV=development babel-node client/webpack/webpack.dev.server.js --progress --colors --profile",
    "test": "NODE_ENV=development babel-node client/webpack/test.server.js --progress --colors --profile",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors --profile --config client/webpack/webpack.config.js && npm-sort"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/spring-react-redux-universal-example.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^6.1.5",
    "babel-plugin-typecheck": "^3.0.0",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "babel-preset-stage-0": "^6.1.2",
    "classnames": "^2.2.0",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "history": "^1.13.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-router": "^1.0.0-beta4",
    "redux-thunk": "^1.0.0",
    "redux-undo": "^0.5.0",
    "serialize-javascript": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.1.4",
    "babel-core": "^6.1.4",
    "babel-loader": "^6.1.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^6.1.4",
    "css-loader": "^0.22.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "json-loader": "^0.5.3",
    "npm-check-updates": "*",
    "npm-sort": "^0.0.4",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-logger": "^2.0.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "4.2.2"
  }
}